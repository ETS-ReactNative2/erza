    "classnames": "^2.2.5",
    "concurrently": "^3.1.0",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "exact": "^0.8.0",
    "express": "^4.14.0",
    "express-graphql": "^0.6.4",
    "graphql": "^0.11.0",
    "graphql-compose": "^3.0.5",
    "graphql-compose-connection": "^3.0.0",
    "graphql-compose-mongoose": "^3.1.1",
